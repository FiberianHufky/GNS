/interface bonding add slaves=qsfpplus1-1,qsfpplus1-2,qsfpplus1-3,qsfpplus1-4 name=bonding1 mode=802.3ad
/interface bonding add slaves=qsfpplus2-1,qsfpplus2-2 name=bonding2 mode=802.3ad
/interface bonding add slaves=qsfpplus2-3,qsfpplus2-4 name=bonding3 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus1,sfp-sfpplus2 name=bonding50 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus3,sfp-sfpplus4 name=bonding51 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus5,sfp-sfpplus6 name=bonding52 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus7,sfp-sfpplus8 name=bonding53 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus9,sfp-sfpplus10 name=bonding54 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus11,sfp-sfpplus12 name=bonding55 mode=802.3ad



######################################### VLAN ########################################

/interface bridge add name=BR vlan-filtering=yes pvid=666

/interface vlan add name=Printers vlan-id=70 disabled=no interface=BR
/interface vlan add name=Guest vlan-id=80 disabled=no interface=BR
/interface vlan add name=BYOD vlan-id=90 disabled=no interface=BR
/interface vlan add name=Company_1 vlan-id=100 disabled=no interface=BR
/interface vlan add name=Blackhole vlan-id=420 disabled=no interface=BR
/interface vlan add name=Default vlan-id=666 disabled=no interface=BR
/interface vlan add name=Management vlan-id=2137 disabled=no interface=BR

/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding1 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding2 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding3 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding50 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding51 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding52 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding53 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding54 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding55 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=ether1 pvid=666

/interface bridge vlan add bridge=BR tagged=BR,ether1,bonding1,bonding2,bonding3,bonding50,bonding51,bonding52,bonding53,bonding54,bonding55 vlan-ids=2137
/interface bridge vlan add bridge=BR tagged=BR,bonding1,bonding2,bonding3,bonding50,bonding51,bonding52,bonding53,bonding54,bonding55 vlan-ids=70,80,90,100


