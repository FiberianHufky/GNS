/interface ethernet set [find default-name=ether1] name=sfp-sfp281
/interface ethernet set [find default-name=ether2] name=sfp-sfp282
/interface ethernet set [find default-name=ether3] name=sfp-sfpplus1
/interface ethernet set [find default-name=ether4] name=sfp-sfpplus2
/interface ethernet set [find default-name=ether5] name=sfp-sfpplus3
/interface ethernet set [find default-name=ether6] name=sfp-sfpplus4
/interface ethernet set [find default-name=ether7] name=sfp-sfpplus5
/interface ethernet set [find default-name=ether8] name=sfp-sfpplus6
/interface ethernet set [find default-name=ether9] name=sfp-sfpplus7
/interface ethernet set [find default-name=ether10] name=sfp-sfpplus8
/interface ethernet set [find default-name=ether11] name=sfp-sfpplus9
/interface ethernet set [find default-name=ether12] name=sfp-sfpplus10
/interface ethernet set [find default-name=ether13] name=sfp-sfpplus11
/interface ethernet set [find default-name=ether14] name=sfp-sfpplus12
/interface ethernet set [find default-name=ether15] name=ether1

/interface bonding add slaves=sfp281,sfp282 name=bonding1 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus1,sfp-sfpplus2 name=bonding2 mode=802.3ad
/interface bonding add slaves=sfp-sfpplus3,sfp-sfpplus4 name=bonding3 mode=802.3ad


################# VLAN ######################


/interface bridge add name=BR vlan-filtering=yes pvid=666

/interface vlan add name=Printers vlan-id=70 disabled=no interface=BR
/interface vlan add name=Guest vlan-id=80 disabled=no interface=BR
/interface vlan add name=BYOD vlan-id=90 disabled=no interface=BR
/interface vlan add name=Company_1 vlan-id=100 disabled=no interface=BR
/interface vlan add name=Blackhole vlan-id=420 disabled=no interface=BR
/interface vlan add name=Default vlan-id=666 disabled=no interface=BR
/interface vlan add name=Management vlan-id=2137 disabled=no interface=BR

/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding1 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding2 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=bonding3 pvid=666
/interface bridge port add bridge=BR frame-types=admit-only-vlan-tagged interface=ether1 pvid=666

/interface bridge vlan add bridge=BR tagged=BR,ether1,bonding1,bonding2,bonding3 vlan-ids=2137
/interface bridge vlan add bridge=BR tagged=BR,bonding1,bonding2,bonding3 vlan-ids=70,80,90,100



############### VRRP ##########################

/interface vrrp add interface=bonding2 vrid=10 priority=100 name=VRRP10
/ip address add address 10.21.37.254 interface=VRRP10


/interface vrrp add interface=bonding2 vrid=70 priority=100 name=VRRP70
/ip address add address 10.70.0.254 interface=VRRP70


##################################### DISABLE INTERFACES ############################################

/interface disable sfp-sfpplus5
/interface disable sfp-sfpplus6
/interface disable sfp-sfpplus7
/interface disable sfp-sfpplus8
/interface disable sfp-sfpplus9
/interface disable sfp-sfpplus10
/interface disable sfp-sfpplus11
/interface disable sfp-sfpplus12


################################# BLACKHOLE VLAN #####################################

/interface bridge add name=BR420
/interface vlan add interface=BR420 name=VLAN420 vlan-id=420

/interface bridge port add bridge=BR420 interface=VLAN420
/interface bridge port add bridge=BR420 interface=sfp-sfpplus5
/interface bridge port add bridge=BR420 interface=sfp-sfpplus6
/interface bridge port add bridge=BR420 interface=sfp-sfpplus7
/interface bridge port add bridge=BR420 interface=sfp-sfpplus8
/interface bridge port add bridge=BR420 interface=sfp-sfpplus9
/interface bridge port add bridge=BR420 interface=sfp-sfpplus10
/interface bridge port add bridge=BR420 interface=sfp-sfpplus11
/interface bridge port add bridge=BR420 interface=sfp-sfpplus12

################################# IP ###################################################
/ip address add address=10.21.37.1/24 interface=Management
